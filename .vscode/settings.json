{
  "eslint.validate": [
    "vue",
    "html",
    "javascript"
  ],
  "[javascript]": {
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false
  },
  "[vue]": {
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "vetur.validation.template": false,
  "vetur.validation.script": false,
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "javascript.format.enable": false,
  "javascript.validate.enable": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/.nuxt": true
  },
  "cSpell.words": [
    "Unprocessable",
    "Vuetify",
    "abcdefghijklmnopqrstuvwxyz",
    "asterixes",
    "awaitjs",
    "axios",
    "chenfengyuan",
    "dbpath",
    "ddns",
    "debounced",
    "destructure",
    "douira",
    "error",
    "esolang",
    "explorative",
    "failed",
    "ixscan",
    "keyup",
    "lpoptions",
    "lpstat",
    "mdash",
    "mdi",
    "munol",
    "muns",
    "nand",
    "nbsp",
    "nuxt",
    "nuxtjs",
    "onselectionchange",
    "op",
    "orgs",
    "pdfs",
    "preamb",
    "qrcode",
    "repo",
    "reql",
    "rgba",
    "roboto",
    "samp",
    "singnum",
    "socketio",
    "sonarjs",
    "styl",
    "truthy",
    "typeof",
    "unbuffered",
    "undefineds",
    "unintuitive",
    "unregistering",
    "unstyled",
    "untick",
    "uuid",
    "uuidv",
    "vuedraggable",
    "vuex",
    "wifi",
    "xnor"
  ]
}
